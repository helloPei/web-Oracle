<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.dao.PaperDao">
	<select id="findAllById" parameterType="int" resultType="com.demo.entity.Paper">
		select TEST_ID, TEST_NAME, START_DATE, END_DATE from ET_TEST where TEST_ID = #{id}
	</select>
</mapper>